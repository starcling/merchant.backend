{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "sourceMap": true,
    "noLib": false,
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "rootDir": "."
  },
  "include": [
    "src/**/*.ts"
  ]
}